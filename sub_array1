#include <iostream>
using namespace std;

int main() {
    int n, x;
    cin >> n >> x;
    int a[2005]; 
    for (int i = 0; i < n; i++) cin >> a[i];

    int sum = 0, ans = 0, left = 0;
    for (int right = 0; right < n; right++) {
        sum += a[right];
        while (sum > x) {
            sum -= a[left];
            left++;
        }
        if (sum == x) ans++;
    }

    cout << ans << "\n";
}
